          "required": true
        },
        "responses": {
          "204": {
            "description": "Successful Response"
          },
          "409": {
            "description": "No test prepared. Call POST /info first.",
            "content": {
              "application/json": {
                "example": {
                  "detail": "No test prepared. Call POST /info first."
                }
              }
            }
          },
          "500": {
            "description": "Failed to update description.",
            "content": {
              "application/json": {
                "example": {
                  "detail": "Failed to update description: Permission denied"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/api/test/finalize": {
      "put": {
        "tags": [
          "test"
        ],
        "summary": "Finalize Test",
        "description": "Finalize a stopped test and move it to history.\n\nChanges state from STOPPED to NOTHING.\nTest data is now part of the historical record and cannot be modified.\n\nRequires that the test has been stopped via PUT /stop first.",
