    "/api/test/running": {
      "get": {
        "tags": [
          "test"
        ],
        "summary": "Get Test State",
        "description": "Get the current state of the test system.\n\nReturns one of four possible states:\n- **NOTHING**: No test is running, no test is stopped, AND no test metadata has been prepared.\n  Ready to start fresh.\n- **PREPARED**: No test is currently running AND no test stopped, BUT test metadata has been set.\n  Ready to call PUT /start to begin the test.\n- **RUNNING**: A test is currently executing and recording data.\n  Data is being collected; call PUT /stop to end recording.\n- **STOPPED**: A test has stopped recording BUT not yet finalized.\n  Review the data, then call PUT /finalize to move to history.",
        "operationId": "get_test_state_api_test_running_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TestStatusResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/test/info": {
      "post": {
        "tags": [
          "test"
        ],
        "summary": "Set Test Info",
        "description": "Set test metadata to prepare for a test run.\n\nThis endpoint stores the test information without starting data collection.\nAfter calling this, the test will be in PREPARED state.\nThen call PUT /start to begin the actual test.",
        "operationId": "set_test_info_api_test_info_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TestMetaData"
              }
            }
          },
          "required": true
        },
        "responses": {
          "204": {
            "description": "Successful Response"
          },
          "409": {
            "description": "A test is already running. Cannot prepare new metadata while a test is in progress.",
            "content": {
              "application/json": {
                "example": {
                  "detail": "A test is already running."
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/api/test/start": {
      "put": {
        "tags": [
          "test"
        ],
        "summary": "Start Test",
        "description": "Start the prepared test and begin recording data.\n\nRequires that metadata has been set via POST /info first.\nChanges state from PREPARED to RUNNING.",
        "operationId": "start_test_api_test_start_put",
        "responses": {
          "204": {
            "description": "Successful Response"
          },
          "400": {
            "description": "No test metadata prepared. Call POST /info first.",
            "content": {
              "application/json": {
                "example": {
                  "detail": "No test metadata prepared. Call POST /info first."
                }
              }
            }
          },
          "409": {
            "description": "A test is already running.",
            "content": {
              "application/json": {
                "example": {
                  "detail": "A test is already running."
                }
              }
            }
          }
        }
      }
    },
    "/api/test/stop": {
      "put": {
        "tags": [
          "test"
        ],
        "summary": "Stop Test",
        "description": "Stop the currently running test and end data recording.\n\nChanges state from RUNNING to STOPPED.\nTest data is preserved in memory and on disk but not yet moved to history.\nCall PUT /finalize to move the test to history.",
        "operationId": "stop_test_api_test_stop_put",
        "responses": {
          "204": {
            "description": "Successful Response"
          }
        }
      }
    },
    "/api/test/description": {
